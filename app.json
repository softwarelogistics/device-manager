{
  "expo": {
    "name": "WiFi Manager",
    "owner": "kevindwolf",
    "slug": "wifi-mgr",
    "version": "1.2.1",
    "orientation": "portrait",
    "icon": "./assets/adaptive-icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/wifiapp.splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "enabled": false
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "plugins": [
      [
        "expo-updates",
        {
          "username": "kevindwolf"
        }
      ],
      "expo-build-properties",
      "react-native-ble-plx"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.nuviot.wifiapp",
      "infoPlist": {
        "NSBluetoothAlwaysUsageDescription": "NuvIoT WiFi Connection Manager uses Bluetooth to communicate such as Green Light Alerting, Kool K9 and SeaWolf Mobile.",
        "LSApplicationQueriesSchemes": [
          "nuviot"
        ],
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.softwarelogistics.devicemgr",
      "permissions": [
        "android.permission.BLUETOOTH",
        "android.permission.BLUETOOTH_ADMIN",
        "android.permission.BLUETOOTH_ADVERTISE",
        "android.permission.BLUETOOTH_CONNECT",
        "android.permission.BLUETOOTH_SCAN",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION"
      ]
    },
    "packagerOpts": {
      "sourceExts": [
        "js",
        "jsx",
        "svgx"
      ],
      "transformer": "node_modules/react-native-svg-transformer/index.js"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "environment": "development",
      "eas": {
        "projectId": "980a83b6-7e96-48b1-af09-e9827a85e453"
      }
    }
  }
}
